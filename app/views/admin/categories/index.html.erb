<section class="admin-products-index">
 
   <header class="page-header">
     <h1>Admin &raquo; All Category</h1>
   </header>
 
   <div class="well">
     <%= link_to '+ New Category', [:new, :admin, :category], class: 'btn btn-success' %>
   </div>
 
   <div class="panel panel-default products">
 
     <table class="table table-bordered">
       <thead>
         <tr>
           <th>ID</th>
           <th>Category</th>
         </tr>
       </thead>
       <tbody>
         <%= render @categories %>
       </tbody>
     </table>
 
   </div>
 
</section>